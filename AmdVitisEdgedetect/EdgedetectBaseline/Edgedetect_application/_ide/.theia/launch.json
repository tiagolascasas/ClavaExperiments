{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Edgedetect_application_sw_emu",
			"debugType": "sw_emu",
			"type": "gdb",
			"request": "launch",
			"target": "${workspaceFolder}/Edgedetect_host/build/x86sim/Edgedetect_host",
			"applicationProjectDir": "${workspaceFolder}/Edgedetect_application",
			"arguments": "${workspaceFolder}/Edgedetect_application/build/sw_emu/hw_link/edgedetect.xclbin ${workspaceFolder}/Edgedetect_host/src/input.bmp ${workspaceFolder}/Edgedetect_host/src/output.bmp",
			"cwd": "${workspaceFolder}/Edgedetect_application/build/sw_emu/Edgedetect_application_sw_emu",
			"gdbpath": "${env:XILINX_VITIS}/bin/xgdb",
			"valuesFormatting": "prettyPrinters",
			"env": {
				"XILINX_VITIS": "${env:XILINX_VITIS}",
				"XILINX_HLS": "${env:XILINX_VITIS}",
				"XCL_EMULATION_MODE": "sw_emu",
				"ENABLE_KERNEL_DEBUG": "true",
				"XCL_BIN_PATH": "${workspaceFolder}/Edgedetect_application/build/sw_emu/package",
				"LD_LIBRARY_PATH": "${env:XILINX_XRT}/lib"
			}
		},
		{
			"type": "tcf-debug",
			"request": "launch",
			"name": "Edgedetect_application_hw_emu",
			"debugType": "tcf_hw_emu",
			"target": {
				"targetConnectionId": "LinuxEmulation",
				"peersIniPath": "../../../.wsdata/.peers.ini",
				"file": "${workspaceFolder}/Edgedetect_host/build/hw/Edgedetect_host",
				"remoteFilePath": "/run/media/mmcblk0p1/Edgedetect_host"
			},
			"env": {
				"XILINX_VITIS": "/run/media/mmcblk0p1",
				"XILINX_XRT": "/usr",
				"XCL_EMULATION_MODE": "hw_emu",
				"XRT_INI_PATH": "/run/media/mmcblk0p1/hw_emu_xrt.ini",
				"LD_LIBRARY_PATH": "/run/media/mmcblk0p1:/tmp:$LD_LIBRARY_PATH"
			},
			"copyToTarget": [
				{
					"source": "${workspaceFolder}/Edgedetect_application/Edgedetect_application_host/runtime/hw_emu_xrt.ini",
					"destination": "/run/media/mmcblk0p1"
				},
				{
					"source": "${workspaceFolder}/Edgedetect_application/build/hw_emu/package/edgedetect.xclbin",
					"destination": "/run/media/mmcblk0p1"
				}
			],
			"copyFromTargetOnTermination": [
				{
					"pattern": ".txt",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw_emu/Edgedetect_application_hw_emu_new"
				},
				{
					"pattern": ".run_summary",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw_emu/Edgedetect_application_hw_emu_new"
				},
				{
					"pattern": ".json",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw_emu/Edgedetect_application_hw_emu_new"
				},
				{
					"pattern": ".csv",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw_emu/Edgedetect_application_hw_emu_new"
				}
			],
			"pathMap": [
				{
					"source": "/home/tls/Dev/wonderland/AmdVitisEdgedetect/EdgedetectFoo/Edgedetect_application/build/hw_emu/hw_link/edgedetect/edgedetect/int/xo/cluster_top_function/cluster_top_function/cpu_sources",
					"destination": "/home/tls/Dev/wonderland/AmdVitisEdgedetect/EdgedetectFoo/Edgedetect_cluster/src"
				}
			],
			"replaceNativeEnv": false,
			"arguments": "./edgedetect.xclbin ./input.bmp ./output.bmp",
			"workingDirectory": "/run/media/mmcblk0p1",
			"stopAtMain": true,
			"stopAtProgramEntry": false,
			"autoAttachProcessChildren": false,
			"internalConsoleOptions": "openOnSessionStart"
		},
		{
			"type": "tcf-debug",
			"request": "launch",
			"name": "Edgedetect_application_hw",
			"debugType": "tcf_hw",
			"target": {
				"targetConnectionId": "Linux Agent",
				"peersIniPath": "../../../.wsdata/.peers.ini",
				"file": "${workspaceFolder}/Edgedetect_host/build/hw/Edgedetect_host",
				"remoteFilePath": "/run/media/mmcblk0p1/Edgedetect_host"
			},
			"env": {
				"XILINX_VITIS": "/run/media/mmcblk0p1",
				"XILINX_XRT": "/usr",
				"XRT_INI_PATH": "/run/media/mmcblk0p1/hw_xrt.ini",
				"LD_LIBRARY_PATH": "/run/media/mmcblk0p1:/tmp:$LD_LIBRARY_PATH"
			},
			"copyToTarget": [
				{
					"source": "${workspaceFolder}/Edgedetect_application/build/hw/package/edgedetect.xclbin",
					"destination": "/run/media/mmcblk0p1"
				},
				{
					"source": "${workspaceFolder}/Edgedetect_application/Edgedetect_application_host/runtime/hw_xrt.ini",
					"destination": "/run/media/mmcblk0p1"
				}
			],
			"copyFromTargetOnTermination": [
				{
					"pattern": ".txt",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw/Edgedetect_application_hw_new"
				},
				{
					"pattern": ".run_summary",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw/Edgedetect_application_hw_new"
				},
				{
					"pattern": ".json",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw/Edgedetect_application_hw_new"
				},
				{
					"pattern": ".csv",
					"destination": "${workspaceFolder}/Edgedetect_application/build/hw/Edgedetect_application_hw_new"
				}
			],
			"pathMap": [
				{
					"source": "/home/tls/Dev/wonderland/AmdVitisEdgedetect/EdgedetectFoo/Edgedetect_application/build/hw/hw_link/edgedetect/edgedetect/int/xo/cluster_top_function/cluster_top_function/cpu_sources",
					"destination": "/home/tls/Dev/wonderland/AmdVitisEdgedetect/EdgedetectFoo/Edgedetect_cluster/src"
				}
			],
			"replaceNativeEnv": false,
			"arguments": "./edgedetect.xclbin",
			"workingDirectory": "/run/media/mmcblk0p1",
			"stopAtMain": true,
			"stopAtProgramEntry": false,
			"autoAttachProcessChildren": false,
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}